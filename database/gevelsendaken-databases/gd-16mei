  KEY `language_lines_group_index` (`group`),
  KEY `language_lines_namespace_index` (`namespace`)
) ENGINE=InnoDB AUTO_INCREMENT=637 DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_unicode_ci;

